{% if obj.name_in_dataset %}
	<h4>{{ obj.name_in_dataset }}</h4>
{% endif %}
{% for k in obj.matched_json %}
    <strong>{{ k }}: </strong>{{ obj.matched_json[k] }}<br/>
{% endfor %}
<hr/>
<strong>{{ _("Імпортовано вперше:") }}</strong> {{ obj.first_updated_from_dataset|date }}<br/>
<strong>{{ _("Імпортовано востаннє:") }}</strong> {{ obj.last_updated_from_dataset|date }}